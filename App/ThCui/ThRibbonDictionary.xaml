                    </adw:RibbonButton>

                    <adw:RibbonButton Name="隔离暖通图层" Text = "隔离暖通图层"
                                    ShowImage="True" ShowText="True" 
                                    Orientation="Vertical" Size="Large"
                                    LargeImage="{StaticResource THMUK}"
                                    Image="{StaticResource THMUK}"
                                CommandParameter="THMUK">
