                                    LargeImage="{StaticResource THLPP}"
                                    Image="{StaticResource THLPP}"
                                CommandParameter="THLPP">
                        <adw:RibbonButton.ToolTip>
                            <adw:RibbonToolTip  
                    Title="给排水用"
                    Content="处理建筑结构提给排水底图的各图层颜色至相应的色号"/>
                        </adw:RibbonButton.ToolTip>
                    </adw:RibbonButton>

