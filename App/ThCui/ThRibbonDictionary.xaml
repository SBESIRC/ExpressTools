                                    Orientation="Vertical" Size="Large"
                                    LargeImage="{StaticResource THMLK}"
                                    Image="{StaticResource THMLK}"
                                CommandParameter="THMLK">
                        <adw:RibbonButton.ToolTip>
                            <adw:RibbonToolTip  
                    Title="锁定暖通图层"
