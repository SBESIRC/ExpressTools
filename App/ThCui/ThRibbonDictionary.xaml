                                    Orientation="Vertical" Size="Large"
                                    LargeImage="{StaticResource THTET}"
                                    Image="{StaticResource THTET}"               
                                    CommandParameter="THTET">
                    <adw:RibbonButton.ToolTip>
                        <adw:RibbonToolTip  
                    Title="综合经济技术指标表"
                            Command="THTET"
