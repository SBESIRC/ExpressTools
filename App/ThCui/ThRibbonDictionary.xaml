                        <adw:RibbonButton.ToolTip>
                            <adw:RibbonToolTip  
                    Title="开启暖通图层"
                    Content="开启所有暖通图层"/>
                        </adw:RibbonButton.ToolTip>
                    </adw:RibbonButton>



                </adw:RibbonSplitButton>
            </adw:RibbonPanelSource>
        </adw:RibbonPanel>

        <adw:RibbonPanel>
            <adw:RibbonPanelSource Title="计算工具">
                <adw:RibbonButton  Name="天华单体规整" Text = "天华单体规整"
