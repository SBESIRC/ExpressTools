                    </adw:RibbonButton>

                    <adw:RibbonButton Name="解锁所有图层" Text = "解锁所有图层"
                                    ShowImage="True" ShowText="True" 
                                    Orientation="Vertical" Size="Large"
                                    LargeImage="{StaticResource THUKA}"
                                    Image="{StaticResource THUKA}"
                                CommandParameter="THUKA">
                        <adw:RibbonButton.ToolTip>
                            <adw:RibbonToolTip  
                    Title="解锁所有图层"
                    Content="解锁所有图层"/>
