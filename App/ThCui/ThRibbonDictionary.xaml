                                        Orientation="Vertical" Size="Large"
                                       LargeImage="{StaticResource TH_NuanTongTuCeng}"
                                        >
                    <adw:RibbonSplitButton.ToolTip>
                        <adw:RibbonToolTip  
                    Title="暖通图层管理"
                    Content="批量操作暖通图层"/>
                    </adw:RibbonSplitButton.ToolTip>

                    <adw:RibbonButton Name="锁定暖通图层" Text = "锁定暖通图层"
