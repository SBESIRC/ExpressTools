                    <adw:RibbonButton.ToolTip>
                        <adw:RibbonToolTip  
                    Title="单体面积汇总"
                            Command="THBAC"
                    Content="汇总单体每层各区域建筑面积和计容面积"/>
                    </adw:RibbonButton.ToolTip>
                </adw:RibbonButton>


