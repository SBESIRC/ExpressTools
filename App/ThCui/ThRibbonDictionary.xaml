                                    LargeImage="{StaticResource THLPM}"
                                    Image="{StaticResource THLPM}"
                                CommandParameter="THLPM">
                        <adw:RibbonButton.ToolTip>
                            <adw:RibbonToolTip  
                    Title="暖通用"
                    Content="处理建筑结构提暖通底图的各图层颜色至相应的色号"/>
                        </adw:RibbonButton.ToolTip>
                    </adw:RibbonButton>

                    <adw:RibbonButton Name="电气用" Text = "电气用"
                                    ShowImage="True" ShowText="True" 
