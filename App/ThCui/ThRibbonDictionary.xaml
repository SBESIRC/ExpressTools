                    </adw:RibbonButton.ToolTip>
                </adw:RibbonButton>

            <adw:RibbonButton  Name="防火分区疏散表" Text = "防火分区疏散表"
                                    ShowImage="True" ShowText="True" 
                                    Orientation="Vertical" Size="Large"
                                    LargeImage="{StaticResource THFET}"
                                    Image="{StaticResource THFET}"               
                                    CommandParameter="THFET">
               <adw:RibbonButton.ToolTip>
                  <adw:RibbonToolTip  
                    Title="防火分区疏散表"
