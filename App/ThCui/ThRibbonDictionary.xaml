                                    Orientation="Vertical" Size="Large"
                                    LargeImage="{StaticResource THBLI}"
                                    Image="{StaticResource THBLI}"
                                CommandParameter="THBLI" >
                    <adw:RibbonButton.ToolTip>
                        <adw:RibbonToolTip  
                    Title="图块集"
                            Command="THBLI"
                    Content="打开图块集工具选项板"/>
                    </adw:RibbonButton.ToolTip>
                </adw:RibbonToggleButton>
                <adw:RibbonButton Name="图块集配置" Text = "图块集配置"
                                    ShowImage="True" ShowText="True" 
                                    Orientation="Vertical" Size="Large"
                                    LargeImage="{StaticResource THBLS}"
                                    Image="{StaticResource THBLS}"
                                CommandParameter="THBLS">
                    <adw:RibbonButton.ToolTip>
                        <adw:RibbonToolTip  
                    Title="图块集配置"
                            Command="THBLS"
                    Content="配置各专业图块集"/>
                    </adw:RibbonButton.ToolTip>
                </adw:RibbonButton>

                <adw:RibbonButton Name="提电气块转换" Text = "提电气块&#xD;转换"
                                    ShowImage="True" ShowText="True" 
                                    Orientation="Vertical" Size="Large"
                                    LargeImage="{StaticResource THBEE}"
                                    Image="{StaticResource THBEE}"
                                CommandParameter="THBEE">
                    <adw:RibbonButton.ToolTip>
                        <adw:RibbonToolTip  
                    Title="提电气块转换"
                            Command="THBEE"
                    Content="将暖通和给排水专业提资给电气的图块转换为电气专业所需的图块"/>
                    </adw:RibbonButton.ToolTip>
                </adw:RibbonButton>

            </adw:RibbonPanelSource>
        </adw:RibbonPanel>

        <adw:RibbonPanel>
            <adw:RibbonPanelSource Title="标注工具">
