    <adw:RibbonTab x:Key="TabXaml" Id="ID_TabMyRibbon"  Title="天华效率工具">
        <adw:RibbonPanel>
            <adw:RibbonPanelSource Title="登录界面">
                <adw:RibbonSplitButton Text="天华" IsSplit="True" IsSynchronizedWithCurrentItem="False"
                                        ShowImage="True" ShowText="True"
                                        Orientation="Vertical" Size="Large"
                                       LargeImage="{StaticResource tianhua}"
                                        >
                    <adw:RibbonButton Name="登录" Text = "登录"
                                    ShowImage="True" ShowText="True" 
                                    Orientation="Vertical" Size="Large"
                                    LargeImage="{StaticResource THCADLOGIN}"
                                    Image="{StaticResource THCADLOGIN}"
                                CommandParameter="THCADLOGIN">
                        <adw:RibbonButton.ToolTip>
                            <adw:RibbonToolTip  
                    Title="登录"
                    Content="登录天华效率平台"/>
                        </adw:RibbonButton.ToolTip>
                    </adw:RibbonButton>
                    <adw:RibbonButton Name="退出" Text = "退出"
                                    ShowImage="True" ShowText="True" 
                                    Orientation="Vertical" Size="Large"
                                    LargeImage="{StaticResource THCADLOGOUT}"
                                    Image="{StaticResource THCADLOGOUT}"
                               CommandParameter="THCADLOGOUT">
                        <adw:RibbonButton.ToolTip>
                            <adw:RibbonToolTip  
                    Title="退出"
                    Content="退出天华效率平台"/>
                        </adw:RibbonButton.ToolTip>
                    </adw:RibbonButton>
                </adw:RibbonSplitButton>
                <adw:RibbonButton Id="ID_BtnHelp" Name="帮助文档" Text = "帮助文档"
                                    ShowImage="True" ShowText="True" 
                                    Orientation="Vertical" Size="Large"
                                    LargeImage="{StaticResource THHLP}"
                                    Image="{StaticResource THHLP}"                              CommandParameter="THHLP">
                    <adw:RibbonButton.ToolTip>
                        <adw:RibbonToolTip  
                            Command="THHLP" Content="获取帮助文档"/>
                    </adw:RibbonButton.ToolTip>
                </adw:RibbonButton>

                <adw:RibbonButton  Name="检查更新" Text = "检查更新"
                                    ShowImage="True" ShowText="True" 
                                    Orientation="Vertical" Size="Large"
                                    LargeImage="{StaticResource THUPT}"
                                    Image="{StaticResource THUPT}"                              CommandParameter="THUPT">
                    <adw:RibbonButton.ToolTip>
                        <adw:RibbonToolTip  
                            Command="THUPT" Content="检查更新"/>
                    </adw:RibbonButton.ToolTip>
                </adw:RibbonButton>


            </adw:RibbonPanelSource>
        </adw:RibbonPanel>

        <adw:RibbonPanel>
            <adw:RibbonPanelSource Title="图块图库">
