<Window x:Class="ThXClip.ThXclipProgressBar"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ThXClip"
        mc:Ignorable="d"
        Title="" Height="80" Width="500">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="500"></ColumnDefinition>            
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="80"></RowDefinition>            
        </Grid.RowDefinitions>
        <ProgressBar HorizontalAlignment="Left" Height="60" Margin="0,0,0,0" Name="pb_import" VerticalAlignment="Top" Width="500"></ProgressBar>
    </Grid> 
</Window>
