<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:ThResourceLibrary"
    >
    
    <!--pay attention to the uri"/"-->
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/ThResourceLibrary;component/Resources/ThButton.xaml"/>
        <ResourceDictionary Source="/ThResourceLibrary;component/Resources/ThGroup.xaml"/>
        <ResourceDictionary Source="/ThResourceLibrary;component/Resources/ThBlockThubnailTemplate.xaml"/>
        <ResourceDictionary Source="/ThResourceLibrary;component/Resources/ThDataGrid.xaml"/>
        <ResourceDictionary Source="/ThResourceLibrary;component/Resources/ThSelectButton.xaml"/>
    </ResourceDictionary.MergedDictionaries>
    
    <ImageBrush
      x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type local:CustomResources}, ResourceId=SadTileBrush}"
      TileMode="Tile"
      ViewportUnits="Absolute" Viewport="0 0 32 32"
      ImageSource="ThResourceLibrary;component/sadface.jpg" Opacity="0.3">
    </ImageBrush>


</ResourceDictionary>